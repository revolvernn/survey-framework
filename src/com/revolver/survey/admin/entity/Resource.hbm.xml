<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.revolver.survey.admin.entity.Resource" table="RESOURCES">
        <id name="resourceId" type="java.lang.Integer">
            <column name="RESOURCE_ID" />
            <generator class="native" />
        </id>
        <property name="actionName" type="java.lang.String">
            <column name="ACTION_NAME"  unique="true" />
        </property>
        <property name="resourceName" type="java.lang.String">
            <column name="RESOURCE_NAME" />
        </property>
        <property name="resPos" type="java.lang.Integer">
            <column name="RES_POS" />
        </property>
        <property name="resCode" type="java.lang.Long">
            <column name="RES_CODE" />
        </property>
    </class>
</hibernate-mapping>
